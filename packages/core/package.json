  "keywords": [
    "claude-code",
    "cc-devkits",
    "utilities",
    "http",
    "retry",
    "cli"
  ],
  "author": "tan-yong-sheng",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tan-yong-sheng/cc-devkits",
    "directory": "packages/core"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
